{
    "dataset_reader": {
        "type": "tsv-reader",
        "token_indexers": {
            "tokens": {
                "type": "single_id"
            }
        },
        "tokenizer": {
            "type": "whitespace"
        }
    },
    "model": {
        "type": "simple_classifier",
        "embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 64
                }
            }
        },
        "encoder": {
            "type": "boe",
            "embedding_dim": 64
        }
    },
    "train_data_path": "./data/train_data.tsv",
    "validation_data_path": "./data/test_data.tsv",
    "trainer": {
        "cuda_device": -1,
        "num_epochs": 100,
        "optimizer": {
            "type": "sgd",
            "lr": 0.01
        },
        "patience": 10
    },
    "data_loader": {
        "batch_size": 32,
        "shuffle": true
    }
}