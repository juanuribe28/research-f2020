{
   "data_loader": {
      "batch_size": 32,
      "sampler": "random",
      "shuffle": false
   },
   "dataset_reader": {
      "token_indexers": {
         "tokens": {
            "type": "single_id"
         }
      },
      "tokenizer": {
         "type": "whitespace"
      },
      "type": "tsv-reader"
   },
   "model": {
      "embedder": {
         "token_embedders": {
            "tokens": {
               "embedding_dim": 64,
               "type": "embedding"
            }
         }
      },
      "encoder": {
         "embedding_dim": 64,
         "type": "boe"
      },
      "type": "simple_classifier"
   },
   "train_data_path": "./data/train_data.tsv",
   "trainer": {
      "cuda_device": -1,
      "num_epochs": 100,
      "optimizer": {
         "type": "adamax"
      },
      "patience": 10
   },
   "validation_data_path": "./data/test_data.tsv"
}
