{
    "dataset_reader": {
        "type": "architecture.dataset_reader.TSVDatasetReader",
        "sentence_token_indexers": {
            "tokens": {
                "type": "single_id"
            }
        },
        "sentence_tokenizer": {
            "type": "whitespace"
        },
        "verb_token_indexers": {
            "tokens": {
                "type": "single_id"
            }
        },
        "verb_tokenizer": {
            "type": "character"
        }
    },
    "model": {
        "type": "dual_simple_classifier",
        "sentence_dropout": 0.2754113309546607,
        "sentence_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 150
                }
            }
        },
        "sentence_encoder": {
            "type": "boe",
            "embedding_dim": 150
        },
        "verb_dropout": 0.568052799709676,
        "verb_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 318
                }
            }
        },
        "verb_encoder": {
            "type": "boe",
            "embedding_dim": 318
        }
    },
    "train_data_path": "./data/train_data.tsv",
    "validation_data_path": "./data/test_data.tsv",
    "trainer": {
        "callbacks": [
            {
                "type": "optuna_pruner"
            }
        ],
        "cuda_device": 0,
        "num_epochs": 100,
        "optimizer": {
            "type": "adamw",
            "lr": 0.011466944888136927
        },
        "patience": 10
    },
    "data_loader": {
        "batch_size": 64,
        "shuffle": true
    }
}