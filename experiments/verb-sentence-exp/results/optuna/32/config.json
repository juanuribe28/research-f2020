{
    "dataset_reader": {
        "type": "architecture.dataset_reader.TSVDatasetReader",
        "sentence_token_indexers": {
            "tokens": {
                "type": "single_id"
            }
        },
        "sentence_tokenizer": {
            "type": "whitespace"
        },
        "verb_token_indexers": {
            "tokens": {
                "type": "single_id"
            }
        },
        "verb_tokenizer": {
            "type": "character"
        }
    },
    "model": {
        "type": "dual_simple_classifier",
        "sentence_dropout": 0.7749853788143265,
        "sentence_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 237
                }
            }
        },
        "sentence_encoder": {
            "type": "boe",
            "embedding_dim": 237
        },
        "verb_dropout": 0.22668460392276613,
        "verb_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 55
                }
            }
        },
        "verb_encoder": {
            "type": "boe",
            "embedding_dim": 55
        }
    },
    "train_data_path": "./data/train_data.tsv",
    "validation_data_path": "./data/test_data.tsv",
    "trainer": {
        "callbacks": [
            {
                "type": "optuna_pruner"
            }
        ],
        "cuda_device": 0,
        "num_epochs": 100,
        "optimizer": {
            "type": "adamw",
            "lr": 0.0010829886263860145
        },
        "patience": 10
    },
    "data_loader": {
        "batch_size": 64,
        "shuffle": true
    }
}