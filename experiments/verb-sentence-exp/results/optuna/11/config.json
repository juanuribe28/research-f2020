{
    "dataset_reader": {
        "type": "architecture.dataset_reader.TSVDatasetReader",
        "sentence_token_indexers": {
            "tokens": {
                "type": "single_id"
            }
        },
        "sentence_tokenizer": {
            "type": "whitespace"
        },
        "verb_token_indexers": {
            "tokens": {
                "type": "single_id"
            }
        },
        "verb_tokenizer": {
            "type": "character"
        }
    },
    "model": {
        "type": "dual_simple_classifier",
        "sentence_dropout": 0.2089599197410086,
        "sentence_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 16
                }
            }
        },
        "sentence_encoder": {
            "type": "boe",
            "embedding_dim": 16
        },
        "verb_dropout": 0.6995786055121762,
        "verb_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 24
                }
            }
        },
        "verb_encoder": {
            "type": "boe",
            "embedding_dim": 24
        }
    },
    "train_data_path": "./data/train_data.tsv",
    "validation_data_path": "./data/test_data.tsv",
    "trainer": {
        "cuda_device": -1,
        "epoch_callbacks": [
            {
                "type": "optuna_pruner"
            }
        ],
        "num_epochs": 100,
        "optimizer": {
            "type": "adamw",
            "lr": 0.005153791119944117
        },
        "patience": 10
    },
    "data_loader": {
        "batch_size": 64,
        "shuffle": true
    }
}